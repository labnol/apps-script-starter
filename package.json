{
  "name": "apps-script-starter",
  "version": "6.1.0",
  "description": "Setup a local development environment for Google Apps Script with Visual Studio Code",
  "author": "Amit Agarwal <amit@labnol.org> (https://www.labnol.org)",
  "homepage": "https://digitalinspiration.com/",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "upload": "clasp push",
    "deploy": "npm run build && npm run upload",
    "deploy:prod": "cross-env NODE_ENV=production npm run deploy",
    "test": "mocha --require esm --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labnol/apps-script-starter.git"
  },
  "keywords": [
    "google-apps-script",
    "google-workspace"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@google/clasp": "^2.3.0",
    "@types/google-apps-script": "^1.0.24",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-labnol": "^1.0.10",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-googleappsscript": "^1.0.3",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esm": "^3.2.25",
    "gas-webpack-plugin": "^1.2.2",
    "mocha": "^8.3.0",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  },
  "bugs": {
    "url": "https://github.com/labnol/apps-script-starter/issues"
  },
  "directories": {
    "test": "test"
  }
}
